    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "hpp": "^0.2.3",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/hpp": "^0.2.1",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.10.1",
    "@types/supertest": "^2.0.10",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2"
  }
